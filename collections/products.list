<!-- Product grid -->
<div id="product-grid" class="container-product">
   <div id="productList" class="row">
   
      {.repeated section items}

       <div id="product-grid-item" class="col-sm-3 col-xs-6">
        <a href="{fullUrl}" class="product">
         <div class="border">

              <div id="">

              <!-- {.repeated section items} -->
                <!-- {.image?} -->
                  <img id="image-count" class="product_item_list img-responsive" {@|image-meta} />
                <!-- {.end} -->
              <!-- {.end} -->

               <!-- this will place the 'SOLD OUT' or 'SALE' flag on the product appropriately -->
              {@|product-status}

            </div>

            <!-- product information, like price and title -->
            <div class="row">
              <div class="col-sm-8 col-xs-12">
                <div class="product-meta">

                  {.section title}

                    <p class="product-title-list">{@}</p>
                  {.end}
                </div>
              </div>

              <div id="price-margin" class="col-sm-4 col-xs-12">
                    {@|product-price}
              </div>

              <!-- <div class="col-sm-12">
                {.excerpt?}
                  <div class="product-excerpt">
                    {excerpt}
                  </div>
                {.end}
              </div> -->

            </div> <!-- end of row -->

            

          </div> <!-- end of border -->
        </a>
      </div> <!-- product-grid-item -->
    {.end}
  </div> <!-- productList -->
</div> <!-- product-grid -->

<!-- Display currency and shipping -->

<div class="currencylist">
  <p>Prices displayed in USD$ <a class="shipping" target="_blank" href="https://william-li-zrps.squarespace.com/shipping">Shipping & Returns</a> </p>
  <a class="shipping-mobile" target="_blank" href="https://william-li-zrps.squarespace.com/shipping">Shipping & Returns</a>
</div>


