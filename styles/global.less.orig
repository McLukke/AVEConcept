.footer-text (@a) when (lightness(@a) >= 60%) {
  color: #16161D;
}

.footer-text (@a) when (lightness(@a) < 60%) {
  color: #fff;
}

.border-color-accent (@a) when (lightness(@a) >= 20%) {
  color: darken(@headerBgColor, 4%);
}

.border-color-accent (@a) when (lightness(@a) < 20%) {
  color: lighten(@headerBgColor, 4%);
}

.border-color-content (@a) when (lightness(@a) >= 20%) {
  border-color: darken(@contentBgColor, 15%);
}

.border-color-content (@a) when (lightness(@a) < 20%) {
  border-color: lighten(@contentBgColor, 10%);
}

.border-color-footer (@a) when (lightness(@a) >= 20%) {
  border-color: darken(@footerBgColor, 15%);
}

.border-color-footer (@a) when (lightness(@a) < 20%) {
  border-color: lighten(@footerBgColor, 10%);
}

.meta-color-active (@a) when (lightness(@a) < 40%) {
  color: lighten(@metaColor, 50%);
}

.meta-color-active (@a) when (lightness(@a) >= 40%) {
  color: darken(@metaColor, 50%);
}

.easeOutQuint(@property, @duration) {
  -webkit-transition: @property @duration cubic-bezier(0.23, 1, 0.32, 1);
  -moz-transition:    @property @duration cubic-bezier(0.23, 1, 0.32, 1);
  -ms-transition:     @property @duration cubic-bezier(0.23, 1, 0.32, 1);
  -o-transition:      @property @duration cubic-bezier(0.23, 1, 0.32, 1);
  transition:         @property @duration cubic-bezier(0.23, 1, 0.32, 1);
}


// MIXINS

// Transitions
// Need to escape multiple transitions: .transition(~"opacity 1s linear, color 1s ease-out");
.transition (@transition) {
  -webkit-transition: @transition;
     -moz-transition: @transition;
       -o-transition: @transition;
          transition: @transition;
}

.box-sizing {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}

.translate (@x, @y:0, @z:0) {
  -webkit-transform:  translate3d(@x, @y, @z);
  -moz-transform:     translate3d(@x, @y, @z);
  -ms-transform:      translate3d(@x, @y, @z);
  -o-transform:       translate3d(@x, @y, @z);
  transform:          translate3d(@x, @y, @z);
}

.transition2 (@transition1, @transition2) {
  -webkit-transition: @transition1, @transition2;
  -moz-transition:    @transition1, @transition2;
  -ms-transition:     @transition1, @transition2;
  -o-transition:      @transition1, @transition2;
  transition:         @transition1, @transition2;
}

