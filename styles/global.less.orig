  color: @descriptionTextColor;
  position: relative;
  z-index: inherit;
  p {
    margin: 0 0 0.75em;
    .page-description-font;
    -webkit-hyphens: auto;
       -moz-hyphens: auto;
            hyphens: auto;
    position: relative;
    z-index: inherit;
    &:last-child {
      margin: 0;
    }
  }
  a, a:visited {
    color: @descriptionTextColor;
    background-color: transparent;
    text-decoration: underline;
  }
}

.extra-description-formatting {

  .page-desc {

    p {
      width: 100%;
    }

    p > strong {
      display: block;
      .desc-bold-font;
      color: @descBoldColor;
      position: relative;
      z-index: inherit;
      -webkit-hyphens: none;
       -moz-hyphens: none;
            hyphens: none;
    }

    p:last-child > a {
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
      .desc-button-font;
      text-decoration: none;
      padding: 1em 1.75em;
      background-color: @descButtonColor;
      display: inline-block;
      line-height: 1em;
      margin: 10px 0 0;
      color: @descButtonTextColor;
      border: none;
      .transition2(background-color 0.1s 0.0s ease-in-out, color 0.1s 0.0s ease-in-out);
      position: relative;
      z-index: inherit;

      &:hover {
        background-color: fade(@descButtonColor, 80%);
      }
    }

    p:empty {
      display: none;
    }
  }

  &.title-background {

    .title-desc-wrapper.over-image.has-main-image,
    &:not(.collection-type-index) .title-desc-wrapper {

      .page-desc {
        font-size: @page-description-font-font-size;
        padding: 1.5em;
      }
    }
  }
}

.extra-description-formatting.description-button-style-outline {
  .page-desc p:last-child > a {
    background-color: transparent;
    border: 2px solid @descButtonColor !important;
    color: @descButtonColor;
    &:hover {
      background-color: @descButtonColor;
      .banner-button-outline-hover-style-guarded-mixin(@descButtonColor);
    }

    /* ----- Guarded mixin to calculate what text color should be on hover ----- */
    .banner-button-outline-hover-style-guarded-mixin (@a) when (lightness(@a) >= 40%) { color: #181818; }
    .banner-button-outline-hover-style-guarded-mixin (@a) when (lightness(@a) < 60%) { color: #efefef; }
  }
}

.extra-description-formatting.description-button-style-raised {
  .page-desc p:last-child > a {
    -webkit-box-shadow: 0px 0.2em 0px 0px darken(@descButtonColor,8%);
    -moz-box-shadow: 0px 0.2em 0px 0px darken(@descButtonColor,8%);
    box-shadow: 0px 0.2em 0px 0px darken(@descButtonColor,8%);
    &:hover {
      background-color: lighten(@descButtonColor,3%);
    }
  }
}

.extra-description-formatting.description-button-corner-style-rounded {
  .page-desc p:last-child > a {
    -webkit-border-radius: 3px;
            border-radius: 3px;
  }
}

.extra-description-formatting.description-button-corner-style-pill {
  .page-desc p:last-child > a {
    -webkit-border-radius: 300px;
            border-radius: 300px;
  }
}

